<launch>

    <!-- launch driver node -->
    <node name="new_driver" pkg="bdd" type="new_driver.py" output="screen"/>

    <!-- launch ZED camera using its ros wrapper --> 
    <include file="$(find zed_wrapper)/launch/zed.launch"/>
    
    <!-- launch car control inputs node -->
    <node name="arduino.py" pkg="bdd" type="arduino.py" output="screen"/>

</launch>
